{
  "expo": {
    "name": "TeAmoMaApp",
    "slug": "TeAmoMaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/screens/assets/heart.png",
    "userInterfaceStyle": "dark",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/screens/assets/heart.png",
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "favicon": "src/screens/assets/heart.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
